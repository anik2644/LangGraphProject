{
  "mcpServers": {
    "duckduckgo-search": {
      "command": "npx",
      "args": ["-y", "duckduckgo-mcp-server"]
    },
    "product-service": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_product"
      ],
      "env": {
        "PRODUCT_API_URL": "http://localhost:8080/api/products/name"
      }
    }
  }
}
